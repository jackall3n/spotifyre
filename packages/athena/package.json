{
  "name": "@spotifyre/athena",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "npx pm2 start pm2.yaml",
    "dev": "tsc-watch --onSuccess \"node ./dist/server.js\"",
    "test": "jest --coverage --verbose",
    "watch": "tsc-watch",
    "start:dev": "ts-node-dev src/server"
  },
  "devDependencies": {
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.13",
    "@types/request-promise": "^4.1.44",
    "@types/supertest": "^2.0.7",
    "@types/swagger-ui-express": "^3.0.1",
    "axios-mock-adapter": "^1.16.0",
    "jest": "^24.8.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tsc-watch": "^2.2.1",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@spotifyre/charis": "^1.0.0",
    "axios": "^0.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "normalizr": "^3.4.0",
    "pm2": "^3.5.1",
    "query-string": "^6.7.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "swagger-ui-express": "^4.0.6",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.40"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "dist",
      ".test.ts"
    ],
    "coverageReporters": [
      "json",
      "html"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
